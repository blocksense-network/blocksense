{
  "name": "@blocksense/avm-relayer",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "AVM Relayer",
  "bin": "./dist/bin.cjs",
  "scripts": {
    "build": "tsup --dts && yarn copy-package-json",
    "start": "yarn node --import tsx ./src/bin.ts",
    "build:ts": "tsup",
    "lint": "yarn run -T eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
    "lint-fix": "yarn lint --fix",
    "test": "vitest run --typecheck --coverage",
    "copy-package-json": "tsx scripts/copy-package-json.ts",
    "changeset-version": "changeset version && node scripts/version.mjs",
    "changeset-publish": "yarn build && TEST_DIST= yarn test && changeset publish"
  },
  "dependencies": {
    "@aztec/accounts": "0.87.9",
    "@aztec/aztec.js": "0.87.9",
    "@aztec/stdlib": "0.87.9",
    "@effect/cli": "0.72.1",
    "@effect/cluster": "^0.53.5",
    "@effect/experimental": "^0.57.4",
    "@effect/platform": "0.93.3",
    "@effect/platform-node": "0.101.1",
    "@effect/printer": "^0.47.0",
    "@effect/printer-ansi": "^0.47.0",
    "@effect/rpc": "^0.72.2",
    "@effect/sql": "^0.48.0",
    "@effect/typeclass": "^0.38.0",
    "effect": "^3.19.6"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@effect/language-service": "^0.56.0",
    "@effect/vitest": "0.27.0",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^3.2.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  }
}
