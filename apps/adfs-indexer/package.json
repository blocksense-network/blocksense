{
  "name": "@blocksense/adfs-indexer",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "description": "Blocksense ADFS Indexer",
  "bin": "./dist/bin.cjs",
  "scripts": {
    "build": "tsup --dts --format esm && yarn copy-package-json",
    "start": "yarn node --import tsx ./src/bin.ts",
    "build:ts": "tsup",
    "lint": "yarn run -T eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
    "lint-fix": "yarn lint --fix",
    "test": "vitest run --typecheck --coverage",
    "copy-package-json": "tsx scripts/copy-package-json.ts",
    "changeset-version": "changeset version && node scripts/version.mjs",
    "changeset-publish": "yarn build && TEST_DIST= yarn test && changeset publish"
  },
  "dependencies": {
    "@blocksense/base-utils": "workspace:^",
    "@blocksense/config-types": "workspace:^",
    "@blocksense/contracts": "workspace:^",
    "@effect/cli": "0.71.0",
    "@effect/cluster": "^0.50.6",
    "@effect/experimental": "^0.56.0",
    "@effect/platform": "0.92.1",
    "@effect/platform-node": "0.98.4",
    "@effect/printer": "^0.46.0",
    "@effect/printer-ansi": "^0.46.0",
    "@effect/rpc": "^0.71.1",
    "@effect/sql": "^0.46.0",
    "@effect/typeclass": "^0.37.0",
    "effect": "^3.18.4",
    "ethers": "6.15.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@effect/language-service": "^0.53.0",
    "@effect/vitest": "0.26.0",
    "@types/node": "^24.9.2",
    "@vitest/coverage-v8": "^3.2.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  }
}
