{
  "name": "@blocksense/data-feeds-config-generator",
  "type": "module",
  "version": "0.0.0",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs",
      "types": "./dist/types/src/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup --silent --config ./rollup.config.mjs",
    "ts": "yarn node --import tsx",
    "get-chainlink-feeds": "yarn ts src/scripts/get_chainlink_feeds.ts",
    "generate-data-feeds-config": "yarn ts src/scripts/generate-feeds-config.ts > artifacts/data-feeds-config-generation-output.json",
    "generate": "yarn get-chainlink-feeds && yarn generate-data-feeds-config"
  },
  "devDependencies": {
    "@blocksense/base-utils": "workspace:*",
    "@effect/data": "^0.18.7",
    "@effect/schema": "^0.72.3",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/node": "^22.5.4",
    "@types/rollup-plugin-peer-deps-external": "^2",
    "effect": "^3.7.2",
    "glob": "^11.0.0",
    "rollup": "^4.21.3",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "yahoo-finance2": "^2.11.3"
  }
}
