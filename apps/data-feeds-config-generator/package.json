{
  "name": "@blocksense/data-feeds-config-generator",
  "type": "module",
  "version": "0.0.0",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs",
      "types": "./dist/types/src/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup --silent --config ./rollup.config.mjs",
    "ts": "yarn node --import tsx",
    "get-chainlink-feeds": "yarn ts src/scripts/get_chainlink_feeds.ts",
    "generate-data-feeds-config": "yarn ts src/scripts/generate-feeds-config.ts",
    "generate": "yarn get-chainlink-feeds && yarn generate-data-feeds-config"
  },
  "devDependencies": {
    "@blocksense/base-utils": "workspace:*",
    "@blocksense/contracts": "workspace:*",
    "@effect/data": "^0.18.7",
    "@effect/schema": "^0.71.1",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/keccak": "^3",
    "@types/node": "^22.3.0",
    "@types/rollup-plugin-peer-deps-external": "^2",
    "effect": "^3.7.0",
    "glob": "^11.0.0",
    "keccak": "^3.0.4",
    "keccak256": "^1.0.6",
    "rollup": "^4.20.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "yahoo-finance2": "^2.11.3"
  },
  "dependencies": {
    "@blocksense/config-types": "workspace:*",
    "web3": "^4.12.1"
  }
}
