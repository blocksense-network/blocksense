{
  "name": "@blocksense/docs.blocksense.network",
  "version": "0.0.0",
  "scripts": {
    "build:deps": "yarn workspace @blocksense/docs-theme build && yarn workspace @blocksense/contracts sol-reflect",
    "build": "yarn build:deps && yarn build:tsc && yarn generate-contracts-mdx && next build",
    "build:tsc": "yarn tsc -p ./tsconfig.lib.json",
    "dev": "next",
    "ready-dev-go": "yarn build:deps && yarn dev",
    "start": "next start",
    "test": "yarn vitest -w false",
    "ts": "yarn node --import tsx",
    "generate-contracts-mdx": "yarn ts ./src/generate-contracts-mdx.ts && yarn prettier --write ./pages/docs/contracts --log-level silent"
  },
  "types": "./src/index.ts",
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "prettier": "^3.3.1",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@blocksense/contracts": "workspace:*",
    "@blocksense/docs-theme": "workspace:*",
    "@blocksense/sol-reflector": "workspace:*",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "autoprefixer": "^10.4.19",
    "next": "^14.2.3",
    "nextra": "^2.13.4",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-vertical-timeline-component": "^3.6.0",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.7.1"
  }
}
