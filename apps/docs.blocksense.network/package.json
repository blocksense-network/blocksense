{
  "name": "@blocksense/docs.blocksense.network",
  "version": "0.0.0",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "yarn build:tsc && vite build",
    "build:tsc": "yarn tsc -p ./tsconfig.lib.json",
    "clean": "yarn clean:normal",
    "clean:normal": "EXCL_NM='-e node_modules' yarn clean:untracked_artifacts",
    "clean:all": "EXCL_NM='' yarn clean:untracked_artifacts",
    "clean:untracked_artifacts": "git clean -fdx -e .env -e .direnv -e .yarn -e .netlify -e .vscode -e .pre-commit-config.yaml $EXCL_NM -- $(git rev-parse --show-toplevel)",
    "dev": "NODE_ENV=development vite",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "test": "yarn vitest -w false"
  },
  "types": "./src/index.ts",
  "devDependencies": {
    "@types/node": "^20.12.12",
    "typescript": "5.2.2",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  }
}
