{
  "name": "@blocksense/docs.blocksense.network",
  "version": "0.0.0",
  "scripts": {
    "build:deps": "yarn workspace @blocksense/docs-theme build && yarn workspace @blocksense/contracts sol-reflect && yarn workspace @blocksense/base-utils build && yarn workspace @blocksense/config-types build",
    "build": "yarn build:deps && yarn build:tsc && yarn generate-contracts-mdx && yarn generate-data-feed-mdx && next build",
    "build:tsc": "yarn tsc -p ./tsconfig.lib.json",
    "dev": "next",
    "dev-watch": "yarn watch:docs-theme & next",
    "ready-dev-go": "yarn build:deps && yarn dev",
    "start": "next start",
    "test": "yarn vitest -w false",
    "ts": "yarn node --import tsx",
    "generate-contracts-mdx": "yarn ts ./src/generate-contracts-mdx.ts && yarn prettier --write ./pages/docs/contracts --log-level silent",
    "generate-data-feed-mdx": "yarn ts ./src/generate-data-feed-mdx.ts && yarn prettier --write ./pages/docs/data-feeds --log-level silent",
    "watch:docs-theme": "nodemon -w ../../libs/docs-theme -e js,jsx,ts,tsx,css --exec 'yarn workspace @blocksense/docs-theme build'"
  },
  "types": "./src/index.ts",
  "devDependencies": {
    "@types/node": "^22.3.0",
    "@types/react": "^18.3.3",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "shiki": "^1.12.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@blocksense/base-utils": "workspace:*",
    "@blocksense/config-types": "workspace:*",
    "@blocksense/contracts": "workspace:*",
    "@blocksense/docs-theme": "workspace:*",
    "@blocksense/monorepo": "workspace:*",
    "@blocksense/sol-reflector": "workspace:*",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@tanstack/react-table": "^8.20.5",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "lucide-react": "^0.427.0",
    "next": "^14.2.5",
    "nextra": "^2.13.4",
    "postcss": "^8.4.41",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-vertical-timeline-component": "^3.6.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.17.0",
    "vaul": "^0.9.1"
  }
}
