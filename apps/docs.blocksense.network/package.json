{
  "name": "@blocksense/docs.blocksense.network",
  "version": "0.0.0",
  "scripts": {
    "build": "yarn build:collect-artifacts && yarn build:tsc && yarn build:nextra && yarn build:pagefind",
    "build:collect-artifacts": "yarn workspace @blocksense/contracts sol-reflect",
    "build:tsc": "yarn tsc -p ./tsconfig.lib.json",
    "build:nextra": "next build",
    "build:pagefind": "pagefind --site .next/server/app --output-path public/_pagefind && cp -r public/_pagefind/ dist/",
    "dev": "next",
    "dev-watch": "yarn watch:docs-theme & next",
    "ready-dev-go": "yarn build:with-deps && yarn dev",
    "start": "next start",
    "test": "vitest run --typecheck --coverage",
    "test:network-icons": "vitest run tests/network-icons.spec.ts",
    "ts": "yarn node --import tsx",
    "watch:docs-theme": "nodemon -w ../../libs/docs-theme -e js,jsx,ts,tsx,css --exec 'yarn workspace @blocksense/docs-theme build'"
  },
  "types": "./src/index.ts",
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@types/react": "^18.3.3",
    "nodemon": "^3.1.10",
    "pagefind": "^1.4.0",
    "prettier": "^3.6.2",
    "shiki": "^2.1.0",
    "tslib": "^2.8.1",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@blocksense/base-utils": "workspace:*",
    "@blocksense/config-types": "workspace:*",
    "@blocksense/contracts": "workspace:*",
    "@blocksense/data-feeds-config-generator": "workspace:*",
    "@blocksense/docs-ui": "workspace:*",
    "@blocksense/nextra-theme-docs": "workspace:*",
    "@blocksense/social-verification": "workspace:*",
    "@blocksense/sol-reflector": "workspace:*",
    "@blocksense/ui": "workspace:*",
    "@effect/platform": "^0.92.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@theguild/remark-mermaid": "^0.3.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "cmdk": "1.1.1",
    "effect": "^3.18.4",
    "fast-glob": "^3.3.3",
    "next": "^15.5.3",
    "nextra": "^4.6.0",
    "postcss": "^8.5.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.1.16",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "vaul": "^1.1.2"
  }
}
