{
  "name": "@blocksense/base-utils",
  "version": "0.0.0",
  "keywords": [
    "utils",
    "helpers"
  ],
  "types": "src/index.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs",
      "types": "./dist/types/index.d.ts"
    },
    "./array-iter": {
      "require": "./dist/cjs/array-iter.cjs",
      "import": "./dist/esm/array-iter.mjs",
      "types": "./dist/types/array-iter.d.ts"
    },
    "./assert": {
      "require": "./dist/cjs/assert.cjs",
      "import": "./dist/esm/assert.mjs",
      "types": "./dist/types/assert.d.ts"
    },
    "./async": {
      "require": "./dist/cjs/async.cjs",
      "import": "./dist/esm/async.mjs",
      "types": "./dist/types/async.d.ts"
    },
    "./buffer-and-hex": {
      "require": "./dist/cjs/buffer-and-hex/index.cjs",
      "import": "./dist/esm/buffer-and-hex/index.mjs",
      "types": "./dist/types/buffer-and-hex/index.d.ts"
    },
    "./fs": {
      "require": "./dist/cjs/fs.cjs",
      "import": "./dist/esm/fs.mjs",
      "types": "./dist/types/fs.d.ts"
    },
    "./evm": {
      "require": "./dist/cjs/evm/index.cjs",
      "import": "./dist/esm/evm/index.mjs",
      "types": "./dist/types/evm/index.d.ts"
    },
    "./env": {
      "require": "./dist/cjs/env/index.cjs",
      "import": "./dist/esm/env/index.mjs",
      "types": "./dist/types/env/index.d.ts"
    },
    "./http": {
      "require": "./dist/cjs/http.cjs",
      "import": "./dist/esm/http.mjs",
      "types": "./dist/types/http.d.ts"
    },
    "./numeric": {
      "require": "./dist/cjs/numeric.cjs",
      "import": "./dist/esm/numeric.mjs",
      "types": "./dist/types/numeric.d.ts"
    },
    "./string": {
      "require": "./dist/cjs/string.cjs",
      "import": "./dist/esm/string.mjs",
      "types": "./dist/types/string.d.ts"
    },
    "./syntax-highlighting": {
      "require": "./dist/cjs/syntax-highlighting.cjs",
      "import": "./dist/esm/syntax-highlighting.mjs",
      "types": "./dist/types/syntax-highlighting.d.ts"
    }
  },
  "scripts": {
    "build": "rollup --silent --config ./rollup.config.mjs",
    "ts": "yarn node --import tsx",
    "test": "yarn vitest --typecheck -w false"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/node": "^22.10.2",
    "@types/rollup-plugin-peer-deps-external": "^2",
    "glob": "^11.0.0",
    "rollup": "^4.28.1",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "vitest": "^2.0.4"
  },
  "dependencies": {
    "@effect/schema": "^0.75.5",
    "effect": "^3.11.7",
    "shiki": "^1.12.1"
  }
}
