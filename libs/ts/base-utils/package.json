{
  "name": "@blocksense/base-utils",
  "version": "0.0.0",
  "keywords": [
    "utils",
    "helpers"
  ],
  "types": "src/index.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs",
      "types": "./src/index.ts"
    },
    "./array-iter": {
      "require": "./dist/cjs/array-iter.cjs",
      "import": "./dist/esm/array-iter.mjs",
      "types": "./src/array-iter.ts"
    },
    "./assert": {
