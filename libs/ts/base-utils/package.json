    "build": "yarn run rollup:build-workspace",
    "ts": "yarn node --import tsx",
    "test": "vitest run --typecheck --coverage"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "3.1.3",
    "glob": "^11.0.0",
    "tsx": "^4.19.2",
    "typescript": "5.8.3",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@effect/platform": "^0.90.0",
