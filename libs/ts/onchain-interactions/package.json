{
  "name": "@blocksense/onchain-interactions",
  "scripts": {
    "ts": "yarn node --import tsx --no-deprecation",
    "get-wallets-balances": "yarn ts src/scripts/balance-script.ts",
    "calculate-cost": "FORCE_COLOR=1 yarn ts src/scripts/cost-calculations.ts \"$@\" | tee cost-calculations.log",
    "unstuck-transaction": "yarn ts src/scripts/unstuck-transaction.ts",
    "check-pending-tx": "yarn ts src/scripts/check-pending-tx.ts",
    "build": "node build.mjs",
    "build:tsc": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "onchain-calculate-cost": "./dist/scripts/cost-calculations.js",
    "onchain-check-pending-tx": "./dist/scripts/check-pending-tx.js",
    "onchain-get-wallets-balances": "./dist/scripts/balance-script.js",
    "onchain-unstuck-transaction": "./dist/scripts/unstuck-transaction.js"
  },
  "dependencies": {
    "@blocksense/base-utils": "workspace:*",
    "@blocksense/contracts": "workspace:*",
    "@types/node": "^24.3.1",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "prom-client": "^15.1.3",
    "viem": "^2.36.0",
    "web3": "^4.15.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/express": "^5",
    "@types/yargs": "^17.0.33",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.0",
    "tsx": "^4.20.5"
  },
  "_originalType": "module"
}
