{
  "name": "@blocksense/monorepo",
  "packageManager": "yarn@4.1.1",
  "workspaces": [
    "libs/*"
  ],
  "scripts": {
    "clean": "git clean -fdx -e .env -e .direnv -e .yarn -e .vscode -- $(git rev-parse --show-toplevel)",
    "format:check": "pre-commit run --all",
    "format:write": "pre-commit run --all || pre-commit run --all"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
