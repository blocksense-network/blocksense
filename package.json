{
  "name": "@blocksense/monorepo",
  "packageManager": "yarn@4.3.1",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "clean": "git clean -fdx -e .env -e .direnv -e .yarn -e .vscode -e .pre-commit-config.yaml -- $(git rev-parse --show-toplevel)",
    "format:check": "pre-commit run --all",
    "format:write": "pre-commit run --all || pre-commit run --all"
  },
  "dependenciesMeta": {
    "@openzeppelin/contracts@5.0.2": {
      "unplugged": true
    },
    "next@14.2.5": {
      "unplugged": true
    }
  },
  "devDependencies": {
    "@vercel/build-utils": "^8.3.5",
    "@vercel/next": "^4.3.6",
    "@vercel/nft": "^0.27.3",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vercel": "^35.2.2"
  }
}
